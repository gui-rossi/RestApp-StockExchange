package net.codejava.ws;

public class SecUpd {
	private String CollectionName;
	private String SoftDeploy;
	private String HardDeploy;
	
	public SecUpd(String collectionName, String softDeploy, String hardDeploy) {
		super();
		CollectionName = collectionName;
		SoftDeploy = softDeploy;
		this.HardDeploy = hardDeploy;
	}
}
